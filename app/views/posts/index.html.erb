

<h3>All of my posts</h3>

  <% @posts.each do |post| %>
  <h3><%= post.title %></h3>
  <p><%= post.body %></p></p>
  <% end %>





<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th>Author</th>
      <th></th>

    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
  
  <%= render "post_summary", :post => post %>
    <% end %>
  </tbody>
</table>

<br>